<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="com.tle.web.htmleditor" version="1">
  <requires>
    <import plugin-id="com.tle.common.i18n" />
    <import plugin-id="com.tle.core.guice" />
    <import plugin-id="com.tle.core.security" />
    <import plugin-id="com.tle.platform.common" />
    <import plugin-id="com.tle.web.freemarker" />
    <import plugin-id="com.tle.web.sections" />
    <import plugin-id="com.tle.web.sections.standard" />
    <import plugin-id="com.equella.base" />
    <import plugin-id="com.equella.core5" />
    <import plugin-id="com.equella.core3" />
    <import plugin-id="com.equella.admin" />
    <import plugin-id="com.equella.serverbase" />
    <import plugin-id="com.equella.core12" />
  </requires>
  <runtime>
    <library type="code" path="classes/" id="core">
      <export prefix="*" />
    </library>
    <library type="resources" path="resources/" id="resources" />
  </runtime>
  <extension-point id="editor">
    <parameter-def id="id" />
    <parameter-def id="class" />
    <parameter-def id="order" type="number" />
  </extension-point>
  <extension-point id="control">
    <parameter-def id="id" />
    <parameter-def id="class" />
    <parameter-def id="order" type="number" />
  </extension-point>
  <extension plugin-id="com.tle.core.guice" point-id="module" id="guiceModules">
    <parameter id="class" value="com.tle.web.htmleditor.HtmlEditorModule" />
  </extension>
  <extension plugin-id="com.tle.common.i18n" point-id="bundle" id="strings">
    <parameter id="file" value="lang/i18n.properties" />
    <parameter id="group" value="resource-centre" />
  </extension>
</plugin>
